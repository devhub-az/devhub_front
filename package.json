{
    "name": "laravel-vue",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite --host",
        "build": "vite build",
        "serve": "vite preview --host --port 3000",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
    },
    "dependencies": {
        "@vue/compiler-sfc": "^3.0.5",
        "axios": "^0.21.2",
        "vite": "^2.6.14",
        "vite-plugin-vue2": "^1.9.0",
        "vue": "^2.6.14",
        "vue-router": "^3.5.3",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.3.4",
        "@vitejs/plugin-vue": "^1.10.1",
        "@tailwindcss/postcss7-compat": "^2.2.4",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@tailwindcss/typography": "^0.4.0",
        "autoprefixer": "^9.8.8",
        "eslint": "^8.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-vue": "8",
        "postcss": "^7.0.36",
        "prettier": "^2.3.2",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
        "vue-template-compiler": "^2.6.14"
    },
    "prettier": {
        "printWidth": 80,
        "arrowParens": "always",
        "semi": false,
        "tabWidth": 4,
        "singleQuote": true,
        "endOfLine": "auto"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "browser": true,
            "es6": true,
            "jest": true,
            "es2021": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
